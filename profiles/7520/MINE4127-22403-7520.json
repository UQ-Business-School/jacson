    "learning_outcomes": [
        {
            "number": "LO1.",
            "description": "DEVELOP A GOOD KNOWLEDGE OF MINE PLANNING - Identify and recognise the key levers for value creation as part of the strategic mine planning process."
        },
        {
            "number": "LO2.",
            "description": "DEVELOP A GOOD KNOWLEDGE OF MINE PLANNING - Use the key levers for value creation to optimise a mine plan within the technical constraints of a mining operation."
        },
        {
            "number": "LO3.",
            "description": "DEVELOP A GOOD KNOWLEDGE OF MINE PLANNING - Appreciate the iterative nature of mine planning."
        },
        {
            "number": "LO4.",
            "description": "DEVELOP A GOOD KNOWLEDGE OF MINE PLANNING - Analyses, evaluate and compare mine plans according to key criteria."
        },
        {
            "number": "LO5.",
            "description": "DEVELOP A GOOD KNOWLEDGE OF MINE PLANNING - Select the most appropriate mine plan and development strategy with justification."
        },
        {
            "number": "LO6.",
            "description": "INCORPORATE OPTIMISATION PROCESSES - Understand and use pit optimisation algorithms including Lerch Grossman and Floating Cone."
        },
        {
            "number": "LO7.",
            "description": "INCORPORATE OPTIMISATION PROCESSES - Understand the capabilities of Operations Research and Mathematical Modelling on solving complex optimisation problems."
        },
        {
            "number": "LO8.",
            "description": "CREATE AND ANALYSE COMPLEX FINANCIAL MODELS - Understand the impacts of time value of money and discounting on mine plan valuation."
        },
        {
            "number": "LO9.",
            "description": "CREATE AND ANALYSE COMPLEX FINANCIAL MODELS - Incorporate taxation, carry forward losses and depreciation into detailed financial modelling."
        },
        {
            "number": "LO10.",
            "description": "KEY DIFFERENCES WITH MINING AND OTHER INDUSTRIES - Understand the role and impact of marginal, total and fixed costs on the mine planning process."
        },
        {
            "number": "LO11.",
            "description": "KEY DIFFERENCES WITH MINING AND OTHER INDUSTRIES - Understand the impact of the exploitation of non-renewable resources on mining company value."
        },
        {
            "number": "LO12.",
            "description": "KEY DIFFERENCES WITH MINING AND OTHER INDUSTRIES - Incorporate key changes into the mine plan in order to mitigate the effect to maximise value the latest technological innovations into the system where appropriate."
        },
        {
            "number": "LO13.",
            "description": "EFFECTIVE COMMUNICATION - Acquire team skills to develop a mining project."
        },
        {
            "number": "LO14.",
            "description": "EFFECTIVE COMMUNICATION - Obtain advanced written and oral communication skills."
        }
    ],
